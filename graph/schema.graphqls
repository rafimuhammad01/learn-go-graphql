scalar Time

type User {
    id: ID!
    username: String!
    password: String!
    email: String!
    name: String!
    created_at: Time!
}

type Query {
    listUser: [User!]!
    getUser(id: ID!) : User!
}

input NewUser {
    username: String!
    password: String!
    email: String!
    name: String!
}

type Mutation {
    createUser(input: NewUser): User
}